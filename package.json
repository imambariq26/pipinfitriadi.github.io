{
  "name": "pipinfitriadi.github.io",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run minify-html",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "minify-html": "html-minifier-terser --input-dir build --output-dir build --file-ext html --collapse-whitespace --remove-comments --minify-css true --minify-js true",
    "update-version": "node update-version.js",
    "release-alpha": "standard-version --prerelease alpha",
    "release-beta": "standard-version --prerelease beta",
    "release-rc": "standard-version --prerelease rc",
    "release": "standard-version"
  },
  "standard-version": {
    "types": [
      {"type": "feat", "section": "Features"},
      {"type": "fix", "section": "Bug Fixes"},
      {"type": "chore", "hidden": true},
      {"type": "docs", "hidden": true},
      {"type": "style", "hidden": true},
      {"type": "refactor", "hidden": true},
      {"type": "perf", "hidden": true},
      {"type": "test", "hidden": true}
    ]
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "html-minifier-terser": "^7.0.0",
    "standard-version": "^9.5.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.18.1"
  },
  "type": "module"
}
